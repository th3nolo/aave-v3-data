# Aave V3 Data Fetcher Configuration
# Copy this file to .env and update with your values

# RPC Monitoring (optional)
ENABLE_RPC_MONITORING=false

# Custom API Keys (optional - demo keys are provided)
# INFURA_API_KEY=your_infura_key_here
# ALCHEMY_API_KEY=your_alchemy_key_here

# GitHub Configuration (for governance monitoring - optional)
# GITHUB_TOKEN=your_github_token_here

# Alert Configuration (optional)
# SMTP_HOST=smtp.gmail.com
# SMTP_PORT=587
# SMTP_USER=your_email@gmail.com
# SMTP_PASSWORD=your_app_password
# ALERT_RECIPIENTS=admin@yourcompany.com

# Performance Settings (optional)
# BATCH_SIZE=50
# REQUEST_TIMEOUT=60
# MAX_RETRIES=3
# CACHE_DURATION=3600

# Logging Level (optional)
# LOG_LEVEL=INFO